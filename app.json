{
  "expo": {
    "name": "HabitFlow",
    "slug": "habitFlow",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "habitflow",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#5CB85C"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.antonysp22.habitflow"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#5CB85C"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.antonysp22.habitflow"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0d601e36-fac6-4c2a-8185-0abe9fceba2d"
      }
    },
    "owner": "antontsp22"
  }
}
